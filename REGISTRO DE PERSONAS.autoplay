<Script>function func_Abrir_RMABD()
	--CHEQUEA SI EXISTE LA BASE DE DATOS INDICADA
	RMABD_Existe = File.DoesExist(_SourceFolder .. "\\RMA.db")
		--COMPRUEBA SI EXISTE O NO
		if RMABD_Existe== true then
			--VARIABLE DONDE GUARDA LA UBICACION DE LA BASE DE DATOS ENCONTRADA
			var_RMABD=(_SourceFolder .. "\\RMA.db")
			--EJECUTA LA ORDEN Y ABRE LA BASE DE DATOS DE LA UBICACION ESTABLECIDA ANTERIORMENTE
			bd=SQLite.Open(var_RMABD)
			
			--COMPRUEBA SI LA ACCION ANTERIOR TUVO ERRORES
			n_ERROR = Application.GetLastError()
				--CHEQUEA SI EL ERROR DEVUELTO NO ES IGUAL A OK
				if n_ERROR ~= 0 then
					--MUESTRA UN MENSAJE YA QUE OBTUVO UN ERROR NO IGUAL A OK
					Dialog.Message("ERROR!", "ERROR DEVUELTO AL ABRIR BASE DE DATOS : " .. SQLite.GetLastErrorString(), MB_OK, MB_ICONSTOP, MB_DEFBUTTON1)
					Application.Exit()
				end
		else
			-- MUESTRA UN MENSAJE DE CREANDO BASE DE DATOS YA QUE NO SE ENCONTRO NINGUNA ANTERIORMENTE
			Dialog.TimedMessage("RADIO MARIA ARGENTINA", "CREANDO NUEVA BASE DE DATOS..." .. " version BD: " .. SQLite.GetVersion(), 5000, MB_ICONINFORMATION);
			--ESTABLECE LA UBICACION DONDE SERA GUARDADA LA BASE DE DATOS NUEVA
			var_RMABD =_SourceFolder .. "\\RMA.db"
			--EJECUTA LA ORDEN Y CREA UNA BASE DE DATOS VACIA EN LA UBICACION ESTABLECIDA ANTERIORMENTE
			bd = SQLite.Open(var_RMABD)	
			
			--CREACION DE BASE DE DATOS POR COMANDOS
			--CREA LA TABLA AREAS
			SQLite.Query(bd, "CREATE TABLE AREAS (ID INTEGER PRIMARY KEY, AREA TEXT)")
			--CREA LA TABLA USUARIOS
			SQLite.Query(bd,"CREATE TABLE  USUARIOS (ID INTEGER PRIMARY KEY, USUARIO TEXT, CLAVE TEXT)")			
			--CREA USUARIO ADMIN
			SQLite.Query(bd, "INSERT INTO USUARIOS VALUES (NULL,'ADMIN', '21232f297a57a5a743894a0e4a801fc3')")
			--CREA LA TABLA LOGIN
			SQLite.Query(bd,"CREATE TABLE  LOGIN (ID INTEGER PRIMARY KEY, USUARIO INTEGER, HORA TIME, FECHA DATE, ESTADO INTEGER)")			
			--CREA LA TABLA PERSONAL 
			SQLite.Query(bd,"CREATE TABLE  PERSONAL (ID INTEGER PRIMARY KEY, APELLIDO TEXT, DNI INTEGER, TIPO TEXT, AREA INTEGER, SUBAREA INTEGER, RECORDATORIO TEXT, FOTO BLOB)")												
			--CREA LA TABLA PLANILLA
			SQLite.Query(bd,"CREATE TABLE  PLANILLA (ID INTEGER PRIMARY KEY, REGISTRADO DATETIME, REGISTRO TEXT, HORA TIME, FECHA DATE, APELLIDO TEXT, DNI INTEGER, TIPO TEXT, AREA TEXT, SUBAREA TEXT, OBSERVACIONES TEXT)")			
			--CREA LA TABLA SUBAREAS
			SQLite.Query(bd,"CREATE TABLE SUBAREAS (ID INTEGER PRIMARY KEY, SUBAREA TEXT, AREA INTEGER)")
		end
end

function func_Cerrar_RMABD()
	-- CIERRA LA BASE DE DATOS CON EL USUARIO GUARDADO ANTERIORMENTE EN LA APLICACION/REGISTRO/BD
	-- SINO LO REGISTRA EN LA BASE DE DATOS
	LOGINUSUARIO = Application.LoadValue("RMA", "USUARIOACTIVO");
		if LOGINUSUARIO=="" then
		--SI NINGUN USUARIO SE LOGUEO EN EL SISTEMA
			SQLite.Close(bd)			
		else
		--ALGUN USUARIO SE LOGUEO EN EL SISTEMA
			func_Registrar_Logout()
			SQLite.Close(bd)			
		end
end

function func_Registrar_Login()
	--REGISTRAR LOGIN EN LA BASE DE DATOS CON EL ESTADO 1:LOGUEADO
		SQLite.Query(bd,"INSERT into PLANILLA VALUES(NULL,'".. System.GetDate(DATE_FMT_ISO) .. " " .. System.GetTime(TIME_FMT_MIL) .."', '" ..  "ENTRADA" .."', '".. System.GetTime(TIME_FMT_HOUR) .. ":" .. System.GetTime(TIME_FMT_MIN) .."', '".. System.GetDate(DATE_FMT_YEAR) .. "-" ..System.GetDate(DATE_FMT_MONTH) .. "-" .. System.GetDate(DATE_FMT_DAY) .."', '".. LOGINUSUARIO .."','" .. "99999999" .. "','".. "--GUARDIA--" .."','" .. "SEGURIDAD" .. "','".. "SEGURIDAD" .. "','" .. "--INICIO DE SESION--" .."')")			
		Dialog.TimedMessage("RMA", "CERRANDO SESION...", 2000, MB_ICONINFORMATION)
			n_ERROR = Application.GetLastError()
				if n_ERROR ~= 0 then
					Dialog.Message("ERROR!", "ERROR DEVUELTO AL ABRIR BASE DE DATOS : " .. SQLite.GetLastErrorString(), MB_OK, MB_ICONSTOP, MB_DEFBUTTON1)
				end
end 

function func_Registrar_Logout()
	--REGISTRA EN LA BASE DE DATOS EL USUARIO QUE FINALIZO LA SESION CON EL ESTADO 0:DESLOGUEADO
	SQLite.Query(bd,"INSERT into PLANILLA VALUES(NULL,'".. System.GetDate(DATE_FMT_EUROPE) .. " " .. System.GetTime(TIME_FMT_MIL) .."', '" ..  "SALIDA" .."', '".. System.GetTime(TIME_FMT_HOUR) .. ":" .. System.GetTime(TIME_FMT_MIN) .."', '".. System.GetDate(DATE_FMT_YEAR) .. "-" ..System.GetDate(DATE_FMT_MONTH) .. "-" .. System.GetDate(DATE_FMT_DAY) .."', '".. LOGINUSUARIO .."','" .. "99999999" .. "','".. "--GUARDIA--" .."','" .. "SEGURIDAD" .. "','".. "SEGURIDAD" .. "','" .. "--CIERRE DE SESION--" .."')")	
	Dialog.TimedMessage("RMA", "CERRANDO SESION...", 2000, MB_ICONINFORMATION)
			n_ERROR = Application.GetLastError()
				if n_ERROR ~= 0 then
					Dialog.Message("ERROR!", "ERROR DEVUELTO AL ABRIR BASE DE DATOS : " .. SQLite.GetLastErrorString(), MB_OK, MB_ICONSTOP, MB_DEFBUTTON1)
				end
end

