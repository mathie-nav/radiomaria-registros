<Event>
<Name>On Click</Name>
<Args/>
<Script>local var_Alta_Apellido = String.Upper(Input.GetText("TXT_ALTA_APELLIDOYNOMBRE"))
local var_Alta_Tareas = ComboBox.GetItemData("CBO_ALTA_TIPO", ComboBox.GetSelected("CBO_ALTA_TIPO"))
local var_Alta_Area = ComboBox.GetItemData("CBO_AREA", ComboBox.GetSelected("CBO_AREA"))
local var_Alta_SubArea = ComboBox.GetItemData("CBO_SUBAREA", ComboBox.GetSelected("CBO_SUBAREA"))
local var_Alta_Dni = Input.GetText("TXT_DNI_ALTA")

sChequeoEx="SELECT APELLIDO from PERSONAL WHERE APELLIDO ='" .. var_Alta_Apellido .. "'"
tChequeoEx = SQLite.QueryToTable(bd, sChequeoEx)

if tChequeoEx and tChequeoEx.Rows &gt; 0 then
		for	i=1,tChequeoEx.Rows do
			var_Alta_Duplicado = tChequeoEx.Data[i]["APELLIDO"]
		end
	Dialog.Message("¡ADVERTENCIA!", "¡EL REGISTRO : [" .. var_Alta_Duplicado .. "] YA SE ENCUENTRA DADO DE ALTA!" , MB_OK, MB_ICONEXCLAMATION, MB_DEFBUTTON1)
	--tChequeoEx.Rows -- PARA SABER CUANTAS VECES ESTA DUPLICADO EL REGISTRO
else
	sQuery="INSERT into PERSONAL VALUES(NULL,'" .. var_Alta_Apellido .. "','" .. var_Alta_Dni .. "','" .. var_Alta_Tareas .. "','" .. var_Alta_Area .. "','" .. var_Alta_SubArea .. "','','NULL')"
	SQLite.Query(bd, sQuery)
	DialogEx.Close(0)
end</Script>
