<Script>function func_Obtener_FechayHora() 
	--OBTIENE DE LA FECHA (DIA,MES Y AÑO) Y LO GUARDA EN VARIABLES
	var_FECHA_DIA = System.GetDate(DATE_FMT_DAY)
	var_FECHA_MES = System.GetDate(DATE_FMT_MONTH)
	var_FECHA_ANIO = System.GetDate(DATE_FMT_YEAR)
	--OBTIENE DE LA HORA (HORA Y MINUTOS) Y LO GUARDA EN VARIABLES
	var_TIEMPO_HORA = System.GetTime(TIME_FMT_HOUR)
	var_TIEMPO_MINUTOS = System.GetTime(TIME_FMT_MIN)
end

function func_Aplicar_FechayHora()
	--APLICA LOS VALORES GUARDADOS ANTERIORMENTE DE FECHA EN LOS COMBOBOX
	ComboBox.SetText("CBO_FECHA_DIA", var_FECHA_DIA)
	ComboBox.SetText("CBO_FECHA_MES", var_FECHA_MES)
	ComboBox.SetText("CBO_FECHA_AÑO", var_FECHA_ANIO)
	--APLICA LOS VALORES GUARDADOS ANTERIORMENTE DE HORA EN LOS COMBOBOX
	ComboBox.SetText("CBO_HORA_HORA", var_TIEMPO_HORA)
	ComboBox.SetText("CBO_HORA_MINUTOS", var_TIEMPO_MINUTOS)
end



