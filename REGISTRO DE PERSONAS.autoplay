<Name>On Show</Name>
<Args/>
<Script>function func_CargarFecha()
	ComboBox.AddItem("CBO_FECHA_AÑO", System.GetDate(DATE_FMT_YEAR), System.GetDate(DATE_FMT_YEAR));
	ComboBox.SetSelected("CBO_FECHA_AÑO", 0)
	ComboBox.AddItem("CBO_FECHA_DIA", System.GetDate(DATE_FMT_DAY), System.GetDate(DATE_FMT_DAY));
	ComboBox.SetSelected("CBO_FECHA_DIA", 0)
	ComboBox.AddItem("CBO_FECHA_MES", System.GetDate(DATE_FMT_MONTH), System.GetDate(DATE_FMT_MONTH));
	ComboBox.SetSelected("CBO_FECHA_MES", 0)
end


COD_INICIO_DE_SESION = DialogEx.Show("FRM_LOGIN", true, nil, nil)

--Dialog.Message("Informacion", "Codigo: " .. COD_INICIO_DE_SESION .. " .", MB_OK, MB_ICONINFORMATION)

if (COD_INICIO_DE_SESION == 1) then
	func_Cargar_CBOApellido()
	func_Cargar_CBOArea()
	func_GRID_Planilla()
	func_OnReloj(true)	
	func_CargarFecha()
else
	func_Bloquear_Controles()
end

