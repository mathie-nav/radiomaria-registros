function func_BindGrid(dbHandle, sGridName, sQuery)
	Grid.SetRedraw(sGridName, false, false)
	tblRS = sQuery
	--chequea si la tabla no es nula
	if tblRS ~= nil then
		--verificar que haya encontrado resultados
		if tblRS.Rows &gt; 0 then
			Grid.SetColumnCount(sGridName, 0);
			Grid.SetRowCount(sGridName, tblRS.Rows+1);
			Grid.SetColumnCount(sGridName, tblRS.Columns)	
			--oculta las tres primeras columnas id, registrado y registro
			Grid.SetColumnWidth(sGridName, 0, 0, false)
			Grid.SetColumnWidth(sGridName, 1, 0, false)
			Grid.SetColumnWidth(sGridName, 2, 0, false)
			
			--establece la fila encabezado
			Grid.SetFixedRowCount(sGridName, 1)
			
			for nCols=1, tblRS.Columns do
				--nombra a la fila encabezado aplicandole los textos de los campos
				Grid.SetCellText(sGridName, 0, nCols -1, tblRS.ColumnNames[nCols], false)
			end
			Grid.SetRowCount(sGridName, tblRS.Rows+1)			
			--le aplica colores cada mitad de celda
			for nRows=1, tblRS.Rows do
				for nCols=1, tblRS.Columns do
					Grid.SetCellText(sGridName, nRows, nCols - 1, tblRS.Data[nRows][tblRS.ColumnNames[nCols]], false)				
					if nRows/2 == Math.Floor(nRows/2) then
						tblColors={Background=16777215,Text=0}
					else
						 	tblColors={Background=16760919,Text=0}						
					end
					Grid.SetCellColors(sGridName, nRows, nCols-1, tblColors, false)			
				end
			end
		else
			nResult = Dialog.Message("RADIO MARIA ARGENTINA", "LA CONSULTA NO HA OBTENIDO NINGUN RESULTADO.", MB_OK, MB_ICONEXCLAMATION, MB_DEFBUTTON1)
		end
	else
		nResult = Dialog.Message("RADIO MARIA ARGENTINA", "LA CONSULTA NO HA OBTENIDO NINGUN RESULTADO.", MB_OK, MB_ICONEXCLAMATION, MB_DEFBUTTON1)
	end
	Grid.SetRedraw(sGridName, true, true)
end

function func_GRID_Planilla()
	local query = "SELECT ID, * FROM PLANILLA ORDER BY ID DESC"
	
		tblReturn = SQLite.QueryToTable(bd, query)
		
		if tblReturn.Rows &gt; 0 then		
			
			Grid.SetColumnCount("GRD_PLANILLA", 0)
			Grid.SetRowCount("GRD_PLANILLA", 0)
			Grid.Refresh("GRD_PLANILLA")	
		
			func_BindGrid(bd, "GRD_PLANILLA", tblReturn)			
			
			Grid.AutoSizeColumns("GRD_PLANILLA", GVS_BOTH, true);
			Grid.ExpandColumnsToFit("GRD_PLANILLA", true, true);
			Grid.SetGridLines("GRD_PLANILLA", true)
			Grid.SetGridLines("GRD_PLANILLA", GVL_BOTH);
			
		else
			Grid.DeleteAllItems("GRD_PLANILLA")
			Grid.ExpandToFit("GRD_PLANILLA", false, true)
			Grid.SetColumnCount("GRD_PLANILLA", 0)
			Grid.SetRowCount("GRD_PLANILLA", 0)
			Grid.Refresh("GRD_PLANILLA")	

			Grid.AutoSizeColumns("GRD_PLANILLA", GVS_BOTH, true);
			Grid.ExpandColumnsToFit("GRD_PLANILLA", true, true);
			Grid.SetGridLines("GRD_PLANILLA", true)
			Grid.SetGridLines("GRD_PLANILLA", GVL_BOTH);	
		end
end

function func_Cargar_CBOArea()
	sQuery = "SELECT * FROM AREAS ORDER BY AREA ASC";
	tQuery = SQLite.QueryToTable(bd,sQuery);	
	--RESETEA EL CONTENIDO DEL COMBOBOX
	ComboBox.ResetContent("CBO_AREA")	
	if tQuery and tQuery.Rows &gt; 0 then
		for i=1,tQuery.Rows do
			cArea = tQuery.Data[i]["AREA"]
			cIDArea = tQuery.Data[i]["ID"]
			ComboBox.AddItem("CBO_AREA", cArea, cIDArea)
		end
	end
	ComboBox.SetSelected("CBO_AREA", 1)
end

function func_Cargar_CBOSubArea() 
	sQuery = "SELECT * FROM SUBAREAS WHERE AREA = '" .. ComboBox.GetItemData("CBO_AREA", ComboBox.GetSelected("CBO_AREA"))  .. "'"
	tQuery = SQLite.QueryToTable(bd,sQuery);
	--RESETEA EL CONTENIDO DEL COMBOBOX
	ComboBox.ResetContent("CBO_SUBAREA")	
	if tQuery and tQuery.Rows &gt; 0 then
		for i=1,tQuery.Rows do
			cSubArea = tQuery.Data[i]["SUBAREA"]
			cIDSubArea = tQuery.Data[i]["ID"]

			ComboBox.AddItem("CBO_SUBAREA", cSubArea, cIDSubArea)
		end
	end
	ComboBox.SetSelected("CBO_SUBAREA", 1)
end

function func_Cargar_CBOApellido() 
	sQuery = "SELECT * FROM PERSONAL ORDER BY APELLIDO ASC";	
	tQuery = SQLite.QueryToTable(bd,sQuery);	
	--RESETEA EL CONTENIDO DEL COMBOBOX
	ComboBox.ResetContent("CBO_APELLIDOYNOMBRE")
	if tQuery and tQuery.Rows &gt; 0 then
		for i=1,tQuery.Rows do
			cApellidoyNombre = tQuery.Data[i]["APELLIDO"]
			cIDApellido = tQuery.Data[i]["ID"]
			--AGREGA LOS ITEMS DE LA BASE DE DATOS CONSULTADA ANTERIORMENTE
			ComboBox.AddItem("CBO_APELLIDOYNOMBRE", cApellidoyNombre, cIDApellido)
		end
	end
	ComboBox.SetSelected("CBO_APELLIDOYNOMBRE", 1)
end</Script>
