<Script>local var_Login_Usuario = String.Upper(Input.GetText("TXT_USUARIO"))
local var_Login_Clave = Crypto.MD5DigestFromString(Input.GetText("TXT_CLAVE"))

function func_Registrar_Logueo()
	--SQLite.Query(bd, "INSERT INTO PLANILLA VALUES (NULL,'datetime','text','time','date','text','int','text','text','text','text','text');")
	--SQLite.Query(bd, "INSERT INTO PLANILLA VALUES (NULL,'" .. System.GetDate(DATE_FMT_EUROPE) .. "','ENTRADA','" .. System.GetTime(TIME_FMT_MIL) .. "','" .. System.GetDate(DATE_FMT_EUROPE) .. "','" .. var_Login_Usuario .."','99999999','--GUARDIA--','SEGURIDAD','---','--INICIO DE SESION--');")
	--SQLite.Query(bd, "INSERT INTO PLANILLA VALUES (NULL, '" .. System.GetDate(DATE_FMT_EUROPE) .. "','" .. "ENTRADA" .. "','" .. System.GetTime(TIME_FMT_MIL) .. "','" .. System.GetDate(DATE_FMT_EUROPE) .. "','" .. var_Login_Usuario .. "','" .. "99999999" .. "','" .. "--GUARDIA--" .. "','" .. "SEGURIDAD" .. "','" .. "SEGURIDAD" .. "','" .. "--INICIO DE SESION--" .. ")")
	--INSERT INTO PLANILLA VALUES (NULL,'30-01-2024','ENTRADA','20:33','30-01-2024','APELLIDO','99999999','GUARDIA','SEGURIDAD','SEGURIDAD','ENTRADA');
	--SQLite.Query(bd, "INSERT INTO PLANILLA VALUES (NULL,'1-01-2024','ENTRADA','20:33','30-01-2024','APELLIDO','99999999','GUARDIA','SEGURIDAD','SEGURIDAD','ENTRADA');");
	
	SQLite.QueryToTable(bd, "INSERT INTO PLANILLA VALUES(NULL,'" .. System.GetDate(DATE_FMT_EUROPE) .. " " .. System.GetTime(TIME_FMT_MIL) .."', '" ..  "ENTRADA" .."', '".. System.GetTime(TIME_FMT_HOUR) .. ":" .. System.GetTime(TIME_FMT_MIN) .."', '".. System.GetDate(DATE_FMT_YEAR) .. "-" ..System.GetDate(DATE_FMT_MONTH) .. "-" .. System.GetDate(DATE_FMT_DAY) .."', '".. var_Login_Usuario .. "','" .. "99999999" .."', '".. "--GUARDIA--" .."','" .. "SEGURIDAD" .. "','" .."SEGURIDAD" .. "','" .. "--INICIO DE SESION--" .."')")
	Application.SaveValue("RMA", "USUARIOACTIVO", var_Login_Usuario)
	IS_INICIO_DE_SESION = IDYES
			n_ERROR = Application.GetLastError()
				if not n_ERROR == SQLite.OK then
					Dialog.Message("ERROR!", "ERROR DEVUELTO AL CONSULTAR LA BASE DE DATOS : " .. SQLite.GetLastErrorString, MB_OK, MB_ICONSTOP, MB_DEFBUTTON1)
				end
end

tbl_Login = SQLite.QueryToTable(bd, "SELECT * FROM USUARIOS WHERE USUARIO = '" .. var_Login_Usuario .. "' AND CLAVE = '" .. var_Login_Clave .. "'")

if tbl_Login.Rows &gt; 0 then
	--ENCONTRO AL USUARIO
	Dialog.TimedMessage("RMA", "INICIANDO SESION ...", 2000, MB_ICONINFORMATION)
	func_Desbloquear_Controles()
	func_Registrar_Logueo()
	COD_INICIO_DE_SESION = DialogEx.Close(1)	
else
	Dialog.Message("ERROR", "USUARIO Y/O CLAVE INCORRECTOS\r\nINGRESE NUEVAMENTE Ó CONTACTE CON UN ADMINISTRADOR.", MB_OK, MB_ICONSTOP, MB_DEFBUTTON1)	
end

</Script>
