<Script>function func_Guardar_Registro()
	--GUARDA LOS VALORES A LA BASE DE DATOS
	SQLite.Query(bd,"INSERT into PLANILLA VALUES(NULL,'".. System.GetDate(DATE_FMT_ISO) .. " " .. System.GetTime(TIME_FMT_MIL) .."', '" ..  ComboBox.GetText("CBO_REGISTRO") .."', '".. ComboBox.GetText("CBO_HORA_HORA") .. ":" .. ComboBox.GetText("CBO_HORA_MINUTOS") .."', '".. ComboBox.GetText("CBO_FECHA_AÑO") .. "-" .. ComboBox.GetItemData("CBO_FECHA_MES", ComboBox.GetSelected("CBO_FECHA_MES")) .. "-" .. ComboBox.GetText("CBO_FECHA_DIA") .."', '".. ComboBox.GetText("CBO_APELLIDOYNOMBRE") .."','" .. Input.GetText("TXT_DNI") .. "', '".. ComboBox.GetText("CBO_TIPO") .."','" .. ComboBox.GetText("CBO_AREA") .. "','".. ComboBox.GetText("CBO_SUBAREA") .. "','" .. Input.GetText("TXT_OBSERVACIONES") .."')")
			--COMPRUEBA SI LA ACCION ANTERIOR TUVO ERRORES
			n_ERROR = Application.GetLastError()
				--CHEQUEA SI EL ERROR DEVUELTO NO ES IGUAL A OK
				if not n_ERROR == SQLite.OK then
					--MUESTRA UN MENSAJE YA QUE OBTUVO UN ERROR NO IGUAL A OK
					Dialog.Message("ERROR!", "ERROR DEVUELTO AL ABRIR BASE DE DATOS : " .. SQLite.GetLastErrorString(), MB_OK, MB_ICONSTOP, MB_DEFBUTTON1)
				end
end

function func_Borrar_Recordatorio()
	var_Apellido_Sel = ComboBox.GetItemData("CBO_APELLIDOYNOMBRE", ComboBox.GetSelected("CBO_APELLIDOYNOMBRE"))
	sQuery="UPDATE PERSONAL SET RECORDATORIO = '' WHERE ID=".. var_Apellido_Sel
	SQLite.Query(bd, sQuery)
end

v_Chequeo_Apellido = ComboBox.GetText("CBO_APELLIDOYNOMBRE")
v_Chequeo_Tarea = ComboBox.GetText("CBO_TIPO")

if v_Chequeo_Apellido == "" then
	Dialog.Message("ERROR", "¡NO SE PERMITE REGISTROS EN BLANCO!\r\nSELECCIONE UNO DE LA LISTA O AGREGUE UNO NUEVO", MB_OK, MB_ICONSTOP, MB_DEFBUTTON1)
else
	if v_Chequeo_Tarea == "RESTRINGIDO" then
		Dialog.Message("¡AVISO!", "¡ESTA PERSONA SE ENCUENTRA EN EL REGISTRO DE RESTRINGIDOS!", MB_OK, MB_ICONSTOP, MB_DEFBUTTON1)
	else
		if CheckBox.GetChecked("CB_RECORDATORIO") == true and not(Input.GetText("TXT_RECORDATORIO") == "") then
			Input.SetText("TXT_OBSERVACIONES",Input.GetText("TXT_OBSERVACIONES") .. " - RECORDATORIO: " .. Input.GetText("TXT_RECORDATORIO"))
			func_Borrar_Recordatorio()
		end
		Input.SetText("TXT_OBSERVACIONES",String.Upper(Input.GetText("TXT_OBSERVACIONES")))
		func_Guardar_Registro()
		func_Cargar_CBOApellido() 		
		func_GRID_Planilla()
		func_Obtener_FechayHora()
		func_Aplicar_FechayHora()
		func_Limpiar_Controles()
	end
end
</Script>
