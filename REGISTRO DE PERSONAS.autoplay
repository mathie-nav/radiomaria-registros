<Script>	var_Apellido_Sel = ComboBox.GetItemData("CBO_APELLIDOYNOMBRE", ComboBox.GetSelected("CBO_APELLIDOYNOMBRE"))

	tbl_Apellido_Sel = SQLite.QueryToTable(bd, "SELECT * FROM PERSONAL WHERE ID = '" .. var_Apellido_Sel .. "'")
	
	var_Id_cbo = tbl_Apellido_Sel.Data[1]["ID"]
	var_Apellido_cbo = tbl_Apellido_Sel.Data[1]["APELLIDO"]
	var_Dni_txt = tbl_Apellido_Sel.Data[1]["DNI"]
	var_Tipo_cbo = tbl_Apellido_Sel.Data[1]["TIPO"]
	var_Area_cbo = tbl_Apellido_Sel.Data[1]["AREA"]
	var_SubArea_cbo = tbl_Apellido_Sel.Data[1]["SUBAREA"]
	var_Recordatorio_cbo = tbl_Apellido_Sel.Data[1]["RECORDATORIO"]
	var_Foto_cbo = tbl_Apellido_Sel.Data[1]["FOTO"]
	
	ComboBox.SetSelected("CBO_TIPO", var_Tipo_cbo)
	Input.SetText("TXT_RECORDATORIO", var_Recordatorio_cbo)
	Input.SetText("TXT_DNI", var_Dni_txt)

	func_Obtener_FechayHora() 
	func_Aplicar_FechayHora() 
	
	if var_Foto_cbo =="NULL" then
		Image.Load("IMG_IMAGENFOTOPERSONAL", "AutoPlay\\Images\\IMG_USUARIOS.png")
	else	
		func_Cargar_Foto()
	end
	
	ComboBox.SetSelected("CBO_AREA",ComboBox.FindItem("CBO_AREA", -1, LB_BYDATA, var_Area_cbo))
	ComboBox.SetSelected("CBO_SUBAREA", ComboBox.FindItem("CBO_SUBAREA", -1, LB_BYDATA, var_SubArea_cbo))
	
	-- TESTEAR SI NO DA PROBLEMAS PARA HACER FOCO AUTOMATICAMENTE Page.SetFocus("CBO_FECHA_DIA")</Script>
