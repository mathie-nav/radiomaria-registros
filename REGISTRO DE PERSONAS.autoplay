<Script>--[[function func_Cargar_CBOArea() 
	sQuery = "SELECT * FROM AREAS ORDER BY AREA ASC";
	tQuery = SQLite.QueryToTable(bd,sQuery);
	
	ComboBox.ResetContent("CBO_AREA")
	if tQuery and tQuery.Rows &gt; 0 then
		for i=1,tQuery.Rows do
			cArea = tQuery.Data[i]["AREA"]
			cIDArea = tQuery.Data[i]["ID"]

			ComboBox.AddItem("CBO_AREA", cArea, cIDArea)
		end
	end
	ComboBox.SetSelected("CBO_AREA", 1)
end--]]</Script>
